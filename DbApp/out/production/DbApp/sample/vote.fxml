<AnchorPane prefHeight="339.0" prefWidth="320.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Vote">
   <children>
      <Label id="welcome-text" alignment="CENTER" layoutY="11.0" prefHeight="33.0" prefWidth="320.0" text="Welcome!" textAlignment="CENTER">
