            <Insets left="2.0" />
         </padding>
      </Label>
      <Label alignment="CENTER" layoutY="64.0" prefHeight="33.0" prefWidth="320.0" text="Please Enter Your Email Address" />
      <JFXComboBox fx:id="candidBox" layoutX="89.0" layoutY="177.0" prefHeight="33.0" prefWidth="143.0" promptText="Choose A Candidate" />
      <JFXButton fx:id="votebtn" buttonType="RAISED" layoutX="110.0" layoutY="226.0" onMouseClicked="#handleVote" prefHeight="33.0" prefWidth="100.0" ripplerFill="#411a1a" text="VOTE" />
      <TextField fx:id="idField" layoutX="86.0" layoutY="108.0" promptText="Your Email Address." />
      <Label alignment="CENTER" layoutY="150.0" prefHeight="25.0" prefWidth="320.0" text="Select Your Candidate" />
      <Button fx:id="logout" layoutX="110.0" layoutY="292.0" mnemonicParsing="false" onMouseClicked="#handleLogOut" prefHeight="33.0" prefWidth="100.0" text="LOG OUT" />
   </children>
    <stylesheets>
        <URL value="@Main.css" />
